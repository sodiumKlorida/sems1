#include <iostream>
using namespace std;

int main()
{
    int angka1, angka2, hasil;

    cout << "input : "; cin >> angka1;
    cout << "input : "; cin >> angka2;

    int temporari = 1;
    for (int i = 1; i <= angka2; i++)
    {
        temporari = temporari * angka1;
    }

    cout << temporari;
}
